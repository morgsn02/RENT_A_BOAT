<h1>MY BOOKINGS!</h1>
<h3>Bookings#my_bookings</h3>
<div>
  <p>Find me in app/views/bookings/my_bookings.html.erb</p>
</div>
<ul>
    <% @my_bookings.each do |booking| %>
      <li><%= link_to booking.boat.name_boat, booking_path(booking) %>
        <%= link_to "Delete",
        booking_path(booking),
        data: {turbo_method: :delete, turbo_confirm: "Are you sure?"}
        %>
      </li>
    <% end %>
</ul>
